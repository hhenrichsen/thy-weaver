    "@rollup/plugin-typescript": "^12.1.1",
    "@swc/core": "^1.9.1",
    "@tauri-apps/cli": "^2.5.0",
    "@types/fs-extra": "^11.0.4",
    "@types/twine-sugarcube": "^2.37.1",
    "browserslist": "^4.24.2",
    "chokidar": "^4.0.1",
    "cross-env": "^7.0.3",
    "fast-glob": "^3.3.2",
    "fs-extra": "^11.2.0",
    "hono": "^4.6.9",
    "lightningcss": "^1.28.1",
    "ora": "^8.1.1",
    "picocolors": "^1.1.1",
    "postcss": "^8.4.47",
    "postcss-import": "^16.1.0",
    "postcss-lightningcss": "^1.0.1",
    "postcss-loader": "^8.1.1",
    "postcss-scss": "^4.0.9",
    "rollup": "^4.24.4",
    "rollup-plugin-postcss": "^4.0.2",
    "sass": "^1.80.6",
